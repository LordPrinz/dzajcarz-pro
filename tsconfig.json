{
	"compilerOptions": {
		"baseUrl": "./",
		"paths": {
			"@/validators/*": ["src/validators/*"],
			"@/models/*": ["src/models/*"],
			"@/lib/*": ["src/lib/*"],
			"@/*": ["src/*"]
		},
		"module": "commonjs",
		"target": "es6",
		"esModuleInterop": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules"]
}
