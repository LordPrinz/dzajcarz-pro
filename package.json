{
  "name": "dzajcarz",
  "module": "src/main.ts",
  "type": "module",
  "description": "Not popular but powerful discord bot.",	
  "author": "LordPrinz",
  "license": "ISC",
  "keywords": [
		"bot",
		"discord",
    "dzajcarz"
	],
  "version": "4.0.0",
  "scripts": {
    "start": "bun run src/main.ts",
    "start:prod": "NODE_ENV=production bun run src/main.ts",
    "test": "bun test",
    "reset:commands": "bun run src/commands/reset.ts",
    "prepare": "bun run scripts/runner.ts --prepare",
    "build": "bun run scripts/runner.ts --build",
    "format": "bun run scripts/runner.ts --format",
    "lint": "bun run scripts/runner.ts --lint",
    "clean": "bun run scripts/runner.ts --clean",
    "dev": "bun run scripts/runner.ts --start:dev",
    "start:dev": "bun run scripts/runner.ts --start:dev",
    "test:cov": "bun run scripts/runner.ts --test:cov",
    "test:watch": "bun run scripts/runner.ts --test:watch",
    "compile": "bun run scripts/runner.ts --compile"
  },

  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "discord.js": "^14.17.3"
  }
}